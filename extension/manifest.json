{
    "manifest_version": 3,
    "name": "ChewTube Cooker",
    "version": "1.0",
    "description": "One-click video submission for ChewTube",
    "permissions": [
        "activeTab",
        "scripting"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": "icon.png"
    },
    "icons": {
        "48": "icon.png",
        "128": "icon.png"
    }
}